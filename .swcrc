{
  "$schema": "https://json.schemastore.org/swcrc",
  "sourceMaps": true,
  "jsc": {
      "parser": {
            "syntax": "typescript",
            "tsx": false,
            "decorators": true
          },
          "target": "es2017",
          "keepClassNames": true,
          "transform": {
            "legacyDecorator": true,
            "decoratorMetadata": true
          },
      "baseUrl": "./"
   },
  "minify": false,
  "module": {
      "type": "commonjs",
      "noInterop": true
    }
}
